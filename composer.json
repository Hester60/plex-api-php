{
  "name": "hester60/plex-api-php",
  "description": "An open souce Plex Server Manager SDK for PHP.",
  "type": "library",
  "keywords": [
    "plex",
    "api",
    "sdk"
  ],
  "scripts": {
    "cs": "./vendor/bin/phpcs --standard=PSR12 src/",
    "cs-fix": "./vendor/bin/phpcbf --standard=PSR12 src/",
    "test": "./vendor/bin/phpunit tests",
    "phpstan": "./vendor/bin/phpstan analyse"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "phpstan/phpstan": "^2.1",
    "squizlabs/php_codesniffer": "^3.11"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Hester60\\PlexApiPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Thomas Carbuccia",
      "email": "thomascarbuccia@hotmail.fr"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.3",
    "guzzlehttp/guzzle": "^7.9"
  }
}
